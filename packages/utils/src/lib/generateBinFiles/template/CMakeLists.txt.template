include("<%= relativeRootPath %><%= cmakeConfigDir %>/<%= workspaceName %>")
set_project_settings(<%= name %> ${CMAKE_CURRENT_SOURCE_DIR})
project(<%= name %> <%= cmakeC %>)
set_binary_settings(<%= name %> ${CMAKE_CURRENT_SOURCE_DIR})
add_subdirectory("<%= relativeRootPath %><%= projectRoot %>" "<%= relativeRootPath %>dist/<%= projectRoot %>/${CMAKE_BUILD_TYPE}")
